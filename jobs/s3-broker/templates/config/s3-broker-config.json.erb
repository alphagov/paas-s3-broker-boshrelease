{
  "port": "<%= p('s3-broker.port') %>",
  "basic_auth_username": "<%= p('s3-broker.basic_auth_username') %>",
  "basic_auth_password": "<%= p('s3-broker.basic_auth_password') %>",
  "log_level": "<%= p('s3-broker.log_level', "DEBUG") %>",
  "aws_region": "<%= p('s3-broker.aws_region') %>",
  "catalog": <%= JSON.dump(p('s3-broker.catalog')) %>,
  "resource_prefix": "<%= p('s3-broker.resource_prefix') %>",
  "iam_user_path": "<%= p('s3-broker.iam_user_path') %>",
  "iam_ip_restriction_policy_arn": "<%= p('s3-broker.iam_ip_restriction_policy_arn') %>",
  "iam_common_user_policy_arn": "<%= p('s3-broker.iam_common_user_policy_arn') %>",
  "iam_user_permissions_boundary_arn": "<%= p('s3-broker.iam_user_permissions_boundary_arn') %>",
  "deploy_env": "<%= p('s3-broker.deploy_environment') %>",

  "tls": {
    "certificate": "<%= p('s3-broker.tls.certificate').gsub(/\n/, '\\n') %>",
    "private_key": "<%= p('s3-broker.tls.private_key').gsub(/\n/, '\\n') %>",
    "ca": "<%= p('s3-broker.tls.ca').gsub(/\n/, '\\n') %>"
  },

  "locket": {
    "address": "<%= p('s3-broker.locket.api_location') %>",
    "ca_cert_file": "/var/vcap/jobs/s3-broker/config/locket.ca_cert.crt",
    "client_cert_file": "/var/vcap/jobs/s3-broker/config/locket.client_cert.crt",
    "client_key_file": "/var/vcap/jobs/s3-broker/config/locket.client_key.key"
  }
}
